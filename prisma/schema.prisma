generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Volunteer {
  id                String   @id @default(auto()) @map("_id") @db.ObjectId
  place_employment    String
  country_residence String
  first_language    String
  first_name        String
  home_country      String
  last_name         String
  medical_speciality String  

  @@map("volunteers")
}
